<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BethanysPieShopMobile.Navigations.PopupView">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Alert Simple" x:Name="btnSimpleA" Clicked="btnSimpleA_Clicked" />
            <Button Text="Alert Question" x:Name="btnQAlert" Clicked="btnQAlert_Clicked"/>
            <Button Text="Alert Multiple" x:Name="btnMultiAlert" Clicked="btnMultiAlert_Clicked"/>
            <Button Text="Alert Input" x:Name="btnInputAlert" Clicked="btnInputAlert_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>